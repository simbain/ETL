        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import com.alibaba.fastjson.JSONArray;
import com.alibaba.fastjson.JSONObject;
import java.security.MessageDigest;
import java.text.SimpleDateFormat;
import java.util.Date;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  if (first) {
    first = false;
  }

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }
  
  r = createOutputRow(r, data.outputRowMeta.size());

  String userid = get(Fields.In, "userid").getString(r);

  String workerid = get(Fields.In, "workerid").getString(r);

  JSONObject jsonObject = new JSONObject();

  JSONObject extattr = new JSONObject();

  JSONArray attrs = new JSONArray();

  JSONObject attr = new JSONObject();

  JSONObject text = new JSONObject();

  text.put("value",workerid);

  attr.put("name","工号");

  attr.put("value",workerid);

  attr.put("type","0");

  attr.put("text",text);

  attrs.add(attr);

  extattr.put("attrs",attrs);

  jsonObject.put("userid",userid);

  jsonObject.put("extattr",extattr);

  get(Fields.Out, "body").setValue(r, jsonObject.toJSONString());

  putRow(data.outputRowMeta, r);

  return true;
}

</class_source>
